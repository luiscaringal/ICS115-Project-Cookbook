<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="0dp"
        android:layout_height="388dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.492"
        app:srcCompat="@drawable/rounded_input" />

    <TextView
        android:id="@+id/closeSpecialty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="9dp"
        android:layout_marginRight="9dp"
        android:layout_marginTop="8dp"
        android:text="Close"
        android:textColor="@color/colorButtonOrder"
        app:layout_constraintBottom_toBottomOf="@+id/imageView17"
        app:layout_constraintEnd_toEndOf="@+id/imageView17"
        app:layout_constraintTop_toTopOf="@+id/imageView17"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/reg_tag1"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/rounded_input_gray"
        android:hint="Tag 1"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/reg_tag2"
        app:layout_constraintEnd_toEndOf="@+id/imageView17"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="@+id/imageView17"
        app:layout_constraintTop_toTopOf="@+id/imageView17"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/reg_tag2"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/rounded_input_gray"
        android:hint="Tag 2"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/reg_tag3"
        app:layout_constraintEnd_toEndOf="@+id/imageView17"
        app:layout_constraintStart_toStartOf="@+id/imageView17"
        app:layout_constraintTop_toBottomOf="@+id/reg_tag1" />

    <EditText
        android:id="@+id/reg_tag3"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/rounded_input_gray"
        android:hint="Tag 3"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/reg_tag4"
        app:layout_constraintEnd_toEndOf="@+id/imageView17"
        app:layout_constraintStart_toStartOf="@+id/imageView17"
        app:layout_constraintTop_toBottomOf="@+id/reg_tag2" />

    <EditText
        android:id="@+id/reg_tag4"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/rounded_input_gray"
        android:hint="Tag 4"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/reg_tag5"
        app:layout_constraintEnd_toEndOf="@+id/imageView17"
        app:layout_constraintStart_toStartOf="@+id/imageView17"
        app:layout_constraintTop_toBottomOf="@+id/reg_tag3" />

    <EditText
        android:id="@+id/reg_tag5"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/rounded_input_gray"
        android:hint="Tag 5"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/reg_tag6"
        app:layout_constraintEnd_toEndOf="@+id/imageView17"
        app:layout_constraintStart_toStartOf="@+id/imageView17"
        app:layout_constraintTop_toBottomOf="@+id/reg_tag4" />

    <EditText
        android:id="@+id/reg_tag6"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/rounded_input_gray"
        android:hint="Tag 6"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/reg_tag7"
        app:layout_constraintEnd_toEndOf="@+id/imageView17"
        app:layout_constraintStart_toStartOf="@+id/imageView17"
        app:layout_constraintTop_toBottomOf="@+id/reg_tag5" />

    <EditText
        android:id="@+id/reg_tag7"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/rounded_input_gray"
        android:hint="Tag 7"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/add_specific_specialty"
        app:layout_constraintEnd_toEndOf="@+id/imageView17"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="@+id/imageView17"
        app:layout_constraintTop_toBottomOf="@+id/reg_tag6" />

    <Button
        android:id="@+id/add_specific_specialty"
        android:layout_width="148dp"
        android:layout_height="35dp"
        android:layout_marginBottom="32dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_button"
        android:fontFamily="@font/montserratsemibold"
        android:text="ADD"
        android:textColor="@color/colorBackgroundInput"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView17"
        app:layout_constraintEnd_toEndOf="@+id/imageView17"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/imageView17"
        app:layout_constraintTop_toBottomOf="@+id/reg_tag7"
        app:layout_constraintVertical_bias="0.496" />

</android.support.constraint.ConstraintLayout>